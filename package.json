{
  "name": "web-booking-scraping",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main/bootstrap.js",
  "scripts": {
    "test": "NODE_ENV=test jest --verbose",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --verbose --collectCoverage",
    "capture:snapshot": "ts-node-dev src/__tests__/captureSnapshot.ts",
    "test:snapshot": "npm run capture:snapshot && npm test",
    "start": "node ./dist/main/bootstrap.js",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn src/main/bootstrap.ts",
    "format": "prettier -uw --cache src/**/*.{js,ts,json,yaml}",
    "lint": "eslint --cache --fix --ext.ts,.js src/**/*",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bull-arena": "^3.0.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.1.1",
    "lint-staged": "^15.2.10",
    "mongodb-memory-server": "^10.0.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@bull-board/express": "^5.23.0",
    "body-parse": "^0.1.0",
    "bull": "^4.16.3",
    "bull-arena": "^4.4.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "module-alias": "^2.2.3",
    "mongoose": "^8.6.0",
    "puppeteer": "^23.2.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
